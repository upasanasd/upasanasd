      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "f=lambda x:np.exp(-np.sin(x**3)/4)\n",
        "fp=lambda x: -3*x*2*np.cos(x*3)/4*f(x)\n",
        "h=0.1\n",
        "fpapprox=[]\n",
        "Eps = np.spacing(1.0)\n",
        "while h >= Eps:\n",
        "  fp1 = (f(1.+h)-f(1.-h))/(2*h)\n",
        "  fpapprox.append([h,fp1,np.abs(fp(1.)-fp1)])\n",
        "  h/=10\n",
        "print(\"---------------------------------------------------------------------\")\n",
        "print(\"h\",\"\\t\\t\\t\",\"Approx Der\",\"\\t\\t\\t\",\"Approx Error\")\n",
        "print(\"---------------------------------------------------------------------\")\n",
        "for x in fpapprox:\n",
        "  print('{0:1.3e}'.format(x[0]),'\\t',\n",
        "        '{0:1.15e}'.format(x[1]),'\\t\\t',\n",
        "        '{0:1.15e}'.format(x[2]))\n",
        "fpapprox = np.array(fpapprox)\n",
        "print(fpapprox)\n",
        "first_column=fpapprox[:,0]\n",
        "second_column=fpapprox[:,2]\n",
        "plt.loglog(first_column,second_column,'r')\n",
        "plt.xlabel(\"h\")\n",
        "plt.ylabel(\"Error\")\n",
        "plt.title(\"Step-size vs Error\")\n",
        "plt.show()"
      ]
    },
